      "source": [
        "Arre=[]\n",
        "print(\"Ingresa la tabla a Guardar: \")\n",
        "tabla=int(input())\n",
        "\n",
        "for i in range(0,10):\n",
        "  resultado=tabla*(i+1)\n",
        "  \n",
        "  Arre.append(resultado)\n",
        "\n",
        "  print(tabla ,\"x\", i+1, \"=\" ,Arre[i])"
      ]
    }
